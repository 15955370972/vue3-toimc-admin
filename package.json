{
  "name": "vue-toimc-admin",
  "private": true,
  "version": "1.0.0-alpha.1",
  "author": {
    "name": "toimc",
    "email": "brian@toimc.com",
    "url": "https://github.com/toimc"
  },
  "scripts": {
    "dev": "vite",
    "build:emit": "vue-tsc --noEmit && vite build",
    "build": "vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.0.1",
    "copy-to-clipboard": "^3.3.1",
    "element-plus": "2",
    "lodash": "^4.17.21",
    "pinia": "^2.0.11",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.70.0",
    "sass": "^1.49.9",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.6.4",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.6",
    "vite-plugin-windicss": "^1.8.3",
    "vue-tsc": "^0.32.1",
    "windicss": "^3.5.1"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "homepage": "https://github.com/toimc-team/vue-toimc-admin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toimc-team/vue-toimc-admin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toimc-team/vue-toimc-admin.git"
  }
}